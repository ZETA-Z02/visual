<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="sql.MainPage">

    <StackLayout>
        <Entry Placeholder="Inputkan email" FontSize="Medium" x:Name="t_email" Keyboard="Email"/>
        <Entry Placeholder="Inputkan password" FontSize="Medium" x:Name="t_password" IsPassword="True"/>

        <Button Text="Login" x:Name="b_login" Clicked="b_login_Clicked"/>
    </StackLayout>

</ContentPage>
